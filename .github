workflows/deploy.yml
name: Deploy Q Auto Trade  
on:  
  push:  
    branches: [ main ]  

jobs:  
  deploy:  
    runs-on: ubuntu-latest  
    steps:  
      - name: Checkout repository  
        uses: actions/checkout@v2  

      - name: Install dependencies  
        run: npm install  

      - name: Deploy to Vercel  
        run: npx vercel --prod
